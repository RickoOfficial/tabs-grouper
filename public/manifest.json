{
	"name": "Tabs grouper with React",
	"description": "Tabs grouper with React",
	"version": "0.0.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"scripting",
		"contextMenus"
	],
	"host_permissions": [
		"*://*/",
		"*://*/*"
	]
}